@import 'jeet'

//Should be moved into master-css file
h2
	font-weight 400
	font-size 2.0em
	color white
	margin: 0
	padding: 15px
a 
	text-decoration: none

p 
	color white

.input-menu
	top: 0;
	position: absolute;
	right: 0;
	bottom 0
	span(1/4)
	min-height: 100%;
	background-color: #368779
	float right
	box-shadow: 2px 0px 5px 2px black;
	-webkit-transition: width 1s ease; /* For Safari 3.1 to 6.0 */
	transition: width 1s ease;

.min-menu
	width 5%
